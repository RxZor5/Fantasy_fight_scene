<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fantasyrollenspiel.Controller.InventoryController">
    <ListView fx:id="inventoryListView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" />
    <Button fx:id="equipButton" layoutX="14.0" layoutY="220.0" mnemonicParsing="false" text="Equip" onAction="#handleEquipButton" />
    <Button layoutX="100.0" layoutY="220.0" mnemonicParsing="false" text="Close" onAction="#handleCloseButton" />
</AnchorPane>
